{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true,
    "moduleResolution": "Bundler",
    "allowJs": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": [
      "react",
      "react-native",
      "react-native/types/modules"
    ]
  },
  "include": [
    "app",
    "App.tsx"
  ],
  "extends": "expo/tsconfig.base"
}
